{
  "manifest_version": 2,
  "name": "Aegis",
  "description": "Provides customizable filtering for websites.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Aegis",
    "default_icon": "images/shield-icon.png"
  },
  "permissions": [
    "tabs"
  ]
}